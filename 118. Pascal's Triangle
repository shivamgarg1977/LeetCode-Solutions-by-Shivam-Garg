class Solution {
    public List<List<Integer>> generate(int numRows) {
        if(numRows==0){
            return new ArrayList<>();
        }
        
        List<List<Integer>> ll=new ArrayList<>();
        
        for(int i=0;i<numRows;i++){
            List<Integer> l=new ArrayList();
            
            for(int j=0;j<=i;j++){
                if(j==0 || j==i){
                    l.add(1);
                }else{
                    l.add(ll.get(i-1).get(j)+ll.get(i-1).get(j-1));
                }
            }
            ll.add(l);
        }
        return ll;
    }
}
