class Solution {
    public boolean isValid(String s) {
       Stack<Character> c=new Stack<>();
        
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)=='('){
                c.push(')');
            }else if(s.charAt(i)=='{'){
                c.push('}');
            }else if(s.charAt(i)=='['){
                c.push(']');
            }else if(c.isEmpty() || c.pop()!=s.charAt(i)){
                return false;
            }
        }
        
        return c.isEmpty();
    }
}
